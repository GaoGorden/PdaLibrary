<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SpinnerView">
        <attr name="spinnertitle" format="string" />
    </declare-styleable>
    <declare-styleable name="ScannerView">
        <attr name="scannertitle" format="string" />
        <attr name="scannerhint" format="string" />
        <attr name="scanner_edit_text" format="string" />
        <attr name="scannerbuttonname" format="string" />
        <attr name="buttonvisable" format="boolean" />
        <attr name="editable" format="boolean" />
        <attr name="inputType" format="enum">
            <enum name="number" value="1" />
            <enum name="password" value="2" />
            <enum name="decimal" value="3" />
            <enum name="none" value="4" />
        </attr>
        <attr name="textChange" format="boolean" />
    </declare-styleable>
    <declare-styleable name="EdittextView">
        <attr name="edittexttitle" format="string" />
        <attr name="edittexthint" format="string" />
        <attr name="edit_edit_text" format="string" />
    </declare-styleable>
    <declare-styleable name="TextshowView">
        <attr name="textshowtitle" format="string" />
        <attr name="show_edit_text" format="string" />
    </declare-styleable>
    <declare-styleable name="NumberView">
        <attr name="numbertitle" format="string" />
        <attr name="numberhint" format="string" />
        <attr name="number_edit_text" format="string" />
        <attr name="numberdefult" format="string" />
    </declare-styleable>
    <declare-styleable name="HeaderTitle">
        <attr name="headertitle" format="string" />
        <attr name="header_right_button_name" format="string" />
        <!--        <attr name="header_right_button_click" format="string"/>-->
        <attr name="headerbuttonvisable" format="boolean" />
    </declare-styleable>
    <declare-styleable name="GridView">
        <attr name="grid_title" format="string" />
        <attr name="grid_row_item_number" format="integer" />
    </declare-styleable>

    <!--tablelayout-->
    <declare-styleable name="TableLayout">
        <attr name="tableMode" format="enum">
            <enum name="auto" value="0" />
            <enum name="equallyDivide" value="1" />
        </attr>
        <attr name="tableRowHeight" format="dimension" />
        <attr name="tableDividerSize" format="dimension" />
        <attr name="tableDividerColor" format="color" />
        <attr name="tableColumnPadding" format="dimension" />
        <attr name="tableTextGravity" format="enum">
            <enum name="center" value="0" />
            <enum name="leftCenter" value="1" />
            <enum name="rightCenter" value="2" />
        </attr>
        <attr name="tableTextSize" format="dimension" />
        <attr name="tableTextColor" format="color" />
        <attr name="tableTextColorSelected" format="color" />
        <attr name="backgroundColorSelected" format="color" />
        <attr name="headerColor" format="color" />
    </declare-styleable>

    <!--表格控件-->
    <declare-styleable name="TableView">
        <attr name="title" format="string" />
        <attr name="columnnumber" format="integer" />
        <attr name="checkVisible" format="boolean" />
        <attr name="textGravity" format="enum">
            <enum name="center" value="0" />
            <enum name="leftCenter" value="1" />
            <enum name="rightCenter" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BarChartView">
        <attr name="text_size_bar" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="PieChartView">
        <attr name="text_size_pie" format="dimension"/>
    </declare-styleable>
</resources>